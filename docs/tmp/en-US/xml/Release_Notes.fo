								 <fo:list-block id="idp19588952" space-before.optimum="0.1pt" space-before.minimum="0.1pt" space-before.maximum="0.1pt" space-after.optimum="1em" space-after.minimum="1em" space-after.maximum="1em" provisional-distance-between-starts="1.2em" provisional-label-separation="0.2em"><fo:list-item id="idp19589112" space-before.optimum="0.1em" space-before.minimum="0.1em" space-before.maximum="0.1em" space-after.optimum="1em" space-after.minimum="1em" space-after.maximum="1em"><fo:list-item-label end-indent="label-end()"><fo:block>1.</fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block margin-left="6pt"><fo:block xmlns="http://www.w3.org/TR/xhtml1/transitional" space-before.optimum="0.1em" space-before.minimum="0.1em" space-before.maximum="0.1em" space-after.optimum="1em" space-after.minimum="1em" space-after.maximum="1em">


<configConfMos
  cookie="%cookie%"
  inHierarchical="false">
  <inConfigs>

    <pair key="%ippooldn%">
      <policyObjectGroup
        descr="%descr%"
        dn="%ippooldn%"
        name="%ippoolname%"
        status="created"/>
    </pair>
    <pair key="%ippooldn%/objgrp-expr-2">
      <policyObjectGroupExpression
        dn="%ippooldn%/objgrp-expr-2"
        id="2"
        opr="eq"
        order="unspecified"
        status="created"/>
    </pair>
    <pair key="%ippooldn%/objgrp-expr-2/nw-ip-2">
      <policyIPAddress
        dataType="string"
        descr=""
        dn="%ippooldn%/objgrp-expr-2/nw-ip-2"
        id="2"
        name=""
        placement="none"
        status="created"
        value="%ipvalue%"/>
    </pair>

  </inConfigs>
</configConfMos>


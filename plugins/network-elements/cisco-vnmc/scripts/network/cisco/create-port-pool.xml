
<configConfMos
  cookie="%cookie%"
  inHierarchical="false">
  <inConfigs>

    <pair key="%portpooldn%">
      <policyObjectGroup
        descr="%descr%"
        dn="%portpooldn%"
        name="%portpoolname%"
        status="created"/>
    </pair>
    <pair key="%portpooldn%/objgrp-expr-2">
      <policyObjectGroupExpression
        dn="%portpooldn%/objgrp-expr-2"
        id="2"
        opr="range"
        order="unspecified"
        status="created"/>
    </pair>
    <pair key="%portpooldn%/objgrp-expr-2/nw-port-2">
      <policyNetworkPort
        appType="Other"
        dataType="string"
        descr=""
        dn="%portpooldn%/objgrp-expr-2/nw-port-2"
        id="2"
        name=""
        placement="begin"
        status="created"
        value="%startport%"/>
    </pair>
    <pair key="%portpooldn%/objgrp-expr-2/nw-port-3">
      <policyNetworkPort
        appType="Other"
        dataType="string"
        descr=""
        dn="%portpooldn%/objgrp-expr-2/nw-port-3"
        id="3"
        name=""
        placement="end"
        status="created"
        value="%endport%"/>
    </pair>

  </inConfigs>
</configConfMos>


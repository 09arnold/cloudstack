
<configConfMos
  cookie="%cookie%"
  inHierarchical="false">
    <inConfigs>
    <pair key="%aclpolicyrefdn%">
      <policyPolicyNameRef
      dn="%aclpolicyrefdn%"
      order="100"
      policyName="%aclpolicyname%"
      status="created"/>
    </pair>
    <pair key="%aclpolicysetdn%">
      <policyPolicySet
      descr=""
      dn="%aclpolicysetdn%"
      name="%aclpolicysetname%"
      status="created"/>
    </pair>
    </inConfigs>
</configConfMos>

